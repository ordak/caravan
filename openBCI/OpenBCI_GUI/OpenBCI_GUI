#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
"$APPDIR/java/bin/java" -Djna.nosys=true -Djava.ext.dirs="$APPDIR/java/lib/ext" -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/LSLLink.jar:$APPDIR/lib/OpenBCI_GUI.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-amd64.jar:$APPDIR/lib/gluegen-rt-natives-linux-amd64.jar:$APPDIR/lib/jsminim.jar:$APPDIR/lib/tritonus_aos.jar:$APPDIR/lib/jl1.0.1.jar:$APPDIR/lib/tritonus_share.jar:$APPDIR/lib/minim.jar:$APPDIR/lib/mp3spi1.9.5.jar:$APPDIR/lib/serial.jar:$APPDIR/lib/jssc.jar:$APPDIR/lib/net.jar:$APPDIR/lib/grafica.jar:$APPDIR/lib/GifAnimation.jar:$APPDIR/lib/oscP5.jar:$APPDIR/lib/udp.jar:$APPDIR/lib/LSLLink.jar:$APPDIR/lib/jna.jar:$APPDIR/lib/jna-platform.jar:$APPDIR/lib/jSerialComm.jar:$APPDIR/lib/brainflow.jar:$APPDIR/lib/commons-logging-1.1.1.jar:$APPDIR/lib/httpmime-4.1.2.jar:$APPDIR/lib/httpclient-cache-4.1.2.jar:$APPDIR/lib/commons-codec-1.4.jar:$APPDIR/lib/httpclient-4.1.2.jar:$APPDIR/lib/httpcore-4.1.2.jar:$APPDIR/lib/httprequests_processing.jar:$APPDIR/lib/controlP5.jar:$APPDIR/lib/openbci_gui_helpers.jar:$APPDIR/lib/ssdp_client.jar" OpenBCI_GUI "$@"
